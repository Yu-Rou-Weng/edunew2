[flake8]

ignore =
    # H401: docstring should not start with a space
    H401,
    # F722: syntax error in forward annotation
    F722,
    # W503: line break before binary operator
    W503

exclude =
    .git,
    __pycache__,
    venv,
    examples,
    migrations,
    autogen/dan_v1
    example/*
    *settings.py

max-line-length = 92
